# JWT Secret (For local dev. Ignore if only using Cognito!)
JWT_SECRET=yourDevSecret123

# AWS Cognito (Authentication)
COGNITO_USER_POOL_ID=us-east-1_xxxxxxxxx
COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxx
COGNITO_REGION=us-east-1

# DynamoDB Table
DYNAMODB_TABLE=QuizMasterTable

# SNS Topic (Notifications)
SNS_TOPIC_ARN=arn:aws:sns:us-east-1:123456789012:QuizReminders

# AWS Region (used by all AWS SDKs)
AWS_REGION=us-east-1

# API Gateway URL (if deployed via Lambda/APIGW)
API_URL=https://xxxxxxxx.execute-api.us-east-1.amazonaws.com/prod

# Port config for local server
PORT=5000

# (Optional) For debugging AWS SDK
DEBUG=false

# (Add other configs as you need)
